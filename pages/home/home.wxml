<!-- 搜索框 -->
<view class="search-container">
  <input class="search-input" type="text" placeholder="情感困扰与关系问题" value="{{keyword}}" bindinput="handleInput"/>
  <button class="search-btn" bindtap="handleSearch">搜索</button>
</view>

<!-- 滚动栏 -->
<swiper class="swiper" indicator-dots=" " autoplay circular>
  <swiper-item>
    <image src="../../icon/刷新.png" class="img1"></image>
  </swiper-item>
  <swiper-item>item2</swiper-item>
  <swiper-item>item3</swiper-item>
</swiper>

