      </view>
      <view>
        <text class="tags"> 情感 </text>
      </view>
    </view>
  </navigator>
  <!-- 第三块区域 -->
  <navigator url="../market/market" class="area">
    <view class="avatar-area">
      <image src="../../image/牢帆.png" class="avatar" />
    </view>
